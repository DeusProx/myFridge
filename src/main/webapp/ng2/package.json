{
  "name": "myfridge",
  "version": "1.0.0",
  "description": "myFridge API helps us to keep a tidy fridge!",
  "private": true,
  "scripts": {
    "cstart": "concurrently --kill-others 'tsc -w' 'live-server --entry-file=index.html --port=8079'",
    "cthneed": "concurrently --kill-others 'tsc -w' 'live-server --entry-file=index.html --host=vm.thneed.de --no-browser --port=8079'",
